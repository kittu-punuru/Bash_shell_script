#!/usr/bin/env python
import urllib2

for url in ["https://dev.chaikinanalytics.com/CPTRestSecure/app/pgr/getPGRValues?uid=9580&tickers=AAPL,GOOG,BCA&token=4XC534118T00FR73S127L77QWU65GA1H","https://dev.chaikinanalytics.com/CPTRestSecure/app/pgr/getPGRValues?uid=9580&tickers=TWTR,RAVE,ZX&token=4XC534118T00FR73S127L77QWU65GA1H"]:
# from try block we get Response 200
# from except block we get response 404
    try:
        connection = urllib2.urlopen(url)
        print connection.getcode()
        connection.close()

    except urllib2.HTTPError, e:
        print e.getcode()    
    

